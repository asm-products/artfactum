<div class="container">
	<div class="row">
		<div class="col-md-6">
		
		<div class="page-header">
			<h4><%= @artwork.title.capitalize %> <small class="pull-right">
      <% if current_user == @artwork.user %>
			<%= link_to "Delete", artwork_path(@artwork), method: :delete %>, <%= link_to "Edit", edit_artwork_path(@artwork) %>
      <% end %>
			</small></h4>
		</div>
		  
		  <a class="thumbnail" href="#">
			  <%= image_tag(@artwork.image_url) %>
			</a>

			<div class="page-header">
				<h4>Inspiration</h4>
			</div>

			<%= @artwork.description %>
		</div>
	</div>
</div>